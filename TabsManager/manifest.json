{
    "manifest_version": 2,
    "name": "Tabs manager",
    "version": "1.0",
    "description": "Tabs manager",
    "icons": {
        "48": "icons/icon_48dp.png"
    },
    "permissions": [
        "tabs"
    ],

    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "browser_style": true,
        "default_icon": "button/beasts.png",
        "default_title": "Tabs Manager",
        "default_popup": "window.html",
        "default_icon": {
            "19": "icons/icon_18dp.png",
            "38": "icons/icon_36dp.png"
        }
    }
}